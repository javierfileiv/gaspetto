{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Arduino Debug (ST-Link)",
      "type": "cortex-debug",
      "request": "launch",
      "servertype": "bmp",
      "device": "STM32F1",
      "cwd": "${workspaceFolder}",
      "executable": "${workspaceFolder}/build/car.ino.elf",
      "interface": "swd",
      "svdFile": "",
      "BMPGDBSerialPort": "/dev/serial/by-id/usb-Black_Magic_Debug_Black_Magic_Probe__ST-Link_v2__v1.10.0-229-g98f11d47_A7864BB1-if00",
      "runToEntryPoint": "setup",
      "gdbPath": "/usr/bin/gdb-multiarch",
      "postLaunchCommands": [
        "cd ${workspaceRoot}/build",
        "target extended-remote /dev/serial/by-id/usb-Black_Magic_Debug_Black_Magic_Probe__ST-Link_v2__v1.10.0-229-g98f11d47_A7864BB1-if00",
        "monitor swdp_scan",
        "attach 1",
        "file car.ino.elf",
        "load",
        "cd ${workspaceRoot}",
        "set mem inaccessible-by-default off",
        "continue"
      ]
    }
  ]
}
